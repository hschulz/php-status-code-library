{
    "name": "hschulz/php-status-code-library",
    "description": "Status codes for protocols and whatnot.",
    "type": "library",
    "keywords": ["php", "statuscode", "status", "code", "protocol", "http", "ftp", "webdav", "smtp"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Hauke Schulz",
            "email": "hauke27@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1.4 || ^8.0.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "hschulz\\StatusCodes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hschulz\\StatusCodes\\Tests\\": "Tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true
    }
}
